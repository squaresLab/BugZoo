# don't do anything with BFD unless objinfo feature enabled
ifeq (objinfo,objinfo)
    ifdef FB_BFD
        FBC_CFLAGS += -d USE_FB_BFD_HEADER
        ifdef BFD_VER
            FBC_CFLAGS += -d __BFD_VER__=$(BFD_VER)
        endif
    endif

    FBC_LIBS = -l bfd -l iberty

    ifeq (linux,cygwin)
        FBC_LIBS += -l intl
    endif

    ifeq (linux,dos)
        FBC_LIBS += -l intl
        # DJGPP 2.03's bfd/iberty require zlib.
        FBC_LIBS += -l z
    endif

    ifeq (linux,freebsd)
        FBC_LIBS += -l intl
    endif

    ifeq (linux,openbsd)
        FBC_LIBS += -l intl
    endif

    ifeq (linux,win32)
        # for libiberty
        FBC_LIBS += -l user32
    endif
endif

